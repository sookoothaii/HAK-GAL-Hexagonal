{
  "workflow_id": "wf-kb-analysis-readonly",
  "version": "1.0.0",
  "ssot_id": "a1b2c3d4e5f6",
  "metadata": {
    "name": "Knowledge Base Analysis (Read-Only)",
    "description": "Safely analyze knowledge base without modifications",
    "author": "Claude-Opus-4.1",
    "created": "2025-01-11T10:00:00Z",
    "tags": ["read-only", "safe", "analysis"],
    "estimated_duration_seconds": 10,
    "risk_level": "none"
  },
  "nodes": [
    {
      "id": "node-start",
      "type": "health_check",
      "label": "System Health",
      "position": {"x": 100, "y": 200},
      "params": {}
    },
    {
      "id": "node-count",
      "type": "get_facts_count",
      "label": "Count Facts",
      "position": {"x": 300, "y": 100},
      "params": {}
    },
    {
      "id": "node-stats",
      "type": "get_predicates_stats",
      "label": "Get Statistics",
      "position": {"x": 300, "y": 300},
      "params": {}
    },
    {
      "id": "node-search-quantum",
      "type": "search_knowledge",
      "label": "Search Quantum",
      "position": {"x": 500, "y": 100},
      "params": {
        "query": "quantum",
        "limit": 10
      }
    },
    {
      "id": "node-search-hypothesis",
      "type": "search_knowledge",
      "label": "Search Hypothesis",
      "position": {"x": 500, "y": 300},
      "params": {
        "query": "hypothesis",
        "limit": 10
      }
    },
    {
      "id": "node-logger",
      "type": "Logger",
      "label": "Log Results",
      "position": {"x": 700, "y": 200},
      "params": {
        "level": "info",
        "message": "Analysis complete"
      }
    }
  ],
  "edges": [
    {"id": "edge-1", "source": "node-start", "target": "node-count"},
    {"id": "edge-2", "source": "node-start", "target": "node-stats"},
    {"id": "edge-3", "source": "node-count", "target": "node-search-quantum"},
    {"id": "edge-4", "source": "node-stats", "target": "node-search-hypothesis"},
    {"id": "edge-5", "source": "node-search-quantum", "target": "node-logger"},
    {"id": "edge-6", "source": "node-search-hypothesis", "target": "node-logger"}
  ],
  "variables": {
    "total_facts": 0,
    "search_results": []
  },
  "execution": {
    "mode": "dag",
    "max_parallel": 3,
    "global_timeout": 60,
    "dry_run": false
  },
  "approvals": {
    "default_policy": "allow_all"
  },
  "outputs": {
    "success": {
      "fact_count": "{{node-count.output}}",
      "statistics": "{{node-stats.output}}",
      "quantum_facts": "{{node-search-quantum.output}}",
      "hypothesis_facts": "{{node-search-hypothesis.output}}"
    }
  }
}